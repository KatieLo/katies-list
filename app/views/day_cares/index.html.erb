<div class="container-fluid">
	<div class="row">
		<div class="col-xs-12 visible-xs">
			<button class="btn btn-primary filter">Filter</button>
		</div>
	</div>

	<div id="test">
	</div>

	<div class="row" id="wrapper">
		<div class="col-xs-12 col-sm-5 col-md-7 left-side">
			<div id="filters">
				<div class="row">
					<div class="hidden-xs col-md-8 filter-label">
						<label>Age Range (Months)</label>
						<!-- age in months -->
						<input id="age-range" type="range" min="1" max="60" step="1" />
						<span class="label-min">1 month</span>
						<span class="label-max">60 months (5 years)</span>
					</div>
				</div>
				<div class="row">
					<div class="hidden-xs col-md-8 filter-label">
						<label>Price Range ($100s)</label>
						<!-- price in $100s -->
						<input id="age-range" type="range" min="500" max="5000" step="100" />
						<span class="label-min">$10</span>
						<span class="label-max">$2000</span>
					</div>
				</div>
				<div class="row">
					<div class="hidden-xs col-md-8 filter-label last">
						<label>Only show daycares with no waitlist</label>
						<input id="age-range" type="checkbox"/>
					</div>
				</div>
			</div>

			<div id="listings">
				<%= react_component 'DayCareListings', { data: @day_cares } %>
			</div>
		</div>
		<div class="hidden-xs col-sm-7 col-md-5" id="map">
			
		</div>
	
	</div>
</div>
